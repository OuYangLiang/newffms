<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://www.springframework.org/schema/context
        http://www.springframework.org/schema/context/spring-context.xsd">

    <bean id="accountService"
        class="com.personal.oyl.newffms.service.impl.AccountServiceImpl" />
        
    <bean id="categoryService"
        class="com.personal.oyl.newffms.service.impl.CategoryServiceImpl" />
        
    <bean id="userProfileService"
        class="com.personal.oyl.newffms.service.impl.UserProfileServiceImpl" />
        
    <bean id="consumptionService"
        class="com.personal.oyl.newffms.service.impl.ConsumptionServiceImpl" />
    
    <bean id="consumptionItemService"
        class="com.personal.oyl.newffms.service.impl.ConsumptionItemServiceImpl" />
    
    <bean id="accountConsumptionService"
        class="com.personal.oyl.newffms.service.impl.AccountConsumptionServiceImpl" />
    
    <bean id="accountAuditService"
        class="com.personal.oyl.newffms.service.impl.AccountAuditServiceImpl" />
        
    <bean id="transactionService"
        class="com.personal.oyl.newffms.service.impl.TransactionServiceImpl" />
        
    <bean id="reportService"
        class="com.personal.oyl.newffms.service.impl.ReportServiceImpl" />
    
</beans>