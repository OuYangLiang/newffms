<?xml version="1.0" encoding="UTF-8"?>
<decorators defaultdir="/WEB-INF/pages/decorator">
	<excludes>
		<pattern>/login</pattern>
	</excludes>

    <decorator name="default" page="main.jsp">
        <pattern>/*</pattern>
    </decorator>
    
    <decorator name="new" page="new-main.jsp">
        <pattern>/report/incoming</pattern>
        <pattern>/report/consumption</pattern>
        
        <pattern>/account/*</pattern>
    </decorator>
</decorators>