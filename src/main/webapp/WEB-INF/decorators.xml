<?xml version="1.0" encoding="UTF-8"?>
<decorators defaultdir="/WEB-INF/pages/decorator">
	<excludes>
		<pattern>/login</pattern>
	</excludes>

    <decorator name="default" page="main.jsp">
        <pattern>/*</pattern>
    </decorator>
    
    <decorator name="new" page="new-main.jsp">
        <pattern>/welcome</pattern>
        <pattern>/report/*</pattern>
        <pattern>/account/*</pattern>
        <pattern>/incoming/*</pattern>
        <pattern>/consumption/*</pattern>
    </decorator>
</decorators>